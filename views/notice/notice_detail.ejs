  <link href="/ndetail_assets/deassets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/ndetail_assets/vendor/icofont/icofont.min.css" rel="stylesheet">
  <link href="/ndetail_assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="/ndetail_assets/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="/ndetail_assets/vendor/venobox/venobox.css" rel="stylesheet">
  <link href="/ndetail_assets/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">
  <link href="/ndetail_assets/vendor/aos/aos.css" rel="stylesheet">

  <link href="/assets/css/style.css" rel="stylesheet">
  <link href="/ndetail_assets/css/style.css" rel="stylesheet">
</head>

<body>

  <% include ../include/header2 %>

  <main id="main" style="margin-right: 120px;">

    <!-- ======= Blog Section ======= -->
    <section id="blog" class="blog" style="margin-bottom: 150px; margin-top: 120px; margin-left: 55px;">
      <div class="container">
        <div>
          <div class="entries">
            <article class="entry entry-single">
              <form action="/notice/delete" method="post">
          
                    <h2 class="entry-title">
                        <%= db_data[0].ntitle %>
                    </h2>

                    <div class="entry-footer clearfix"></div>

                    <div class="entry-meta" style="margin-top: 15px;">
                        <ul>
                          <li class="d-flex align-items-center"><i class="icofont-user"></i> <%= db_data[0].nwriter %></li>
                          <li class="d-flex align-items-center"><i class="icofont-wall-clock"></i> <time datetime="2020-01-01"><%= dayjs(db_data[0].ndate).format('YY.MM.DD') %></time></li>
                        </ul>
                    </div>

                    <div class="entry-content">
                        <blockquote>
                        <i class="icofont-quote-left quote-left"></i>
                        <p>
                          <%- db_data[0].ncontent %>
                        </p>
                        <i class="las la-quote-right quote-right"></i>
                        <i class="icofont-quote-right quote-right"></i>
                        </blockquote>
                    </div>

                    <div class="entry-footer clearfix">
                        <div class="float-left">
                        </div>
                  </div>
              </article><!-- End blog entry -->
              
          </div>
        </div>
        <div class="text-end">
            <button type="button" class="btn btn-outline-primary d-none" id="btnSave" onclick="location.href='/notice/update/<%= db_data[0].nid %>'">글 수정</button>
            <button type="submit" class="btn btn-outline-danger d-none" id="btnCancel">글 삭제</button>
            
            <input type="hidden" name="nidx" value="<%= db_data[0].nid %>">
            <button type="button" class="btn btn-outline-dark" id="btnList" onclick="location.href='/notice/1'">목록</button>
          </form>  
        </div>
      </div>
    </section><!-- End Blog Section -->

  </main><!-- End #main -->
</body>

   <!-- Vendor JS Files -->
  <script src="/assets/vendor/jquery/jquery.min.js"></script>
  <script src="/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/assets/vendor/jquery.easing/jquery.easing.min.js"></script>
  <script src="/assets/vendor/php-email-form/validate.js"></script>
  <script src="/assets/vendor/waypoints/jquery.waypoints.min.js"></script>
